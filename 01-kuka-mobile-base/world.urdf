<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 0.0">

	<robot name="Kuka-IIWA">
		<!-- model node is required -->
		<model dir="../resources/01-kuka-mobile-base" path="iiwa7.urdf" name="kuka_iiwa" />
		<!-- origin node is optional -->
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	</robot>

	<robot name="robotObject">
		<!-- model node is required -->
		<model dir="../resources/01-kuka-mobile-base" path="robotObject.urdf" name="robotObject" />
		<!-- origin node is optional -->
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Floor">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
		<!-- <collision>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <box size="3.0 3.0 0.1" />
	        </geometry>
	    </collision> -->
	    <visual>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <box size="3.0 3.0 0.1" />
	        </geometry>
	        <material name="material_red">
		        <color rgba="0.1 0.1 0.1 1.0" />
	        </material>
	    </visual>
	</static_object>

	<static_object name="box">
		<origin xyz="0.0 -1.0 0.0" rpy="0 0 0" />
		<!-- <collision>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <box size="3.0 3.0 0.1" />
	        </geometry>
	    </collision> -->
	    <visual>
	        <origin xyz="0.0 0.0 0.2" rpy="0 0 0" />
	        <geometry>
	            <box size="0.3 0.3 0.4" />
	        </geometry>
	        <material name="material_brown">
		        <color rgba="0.2862 0.1921 0.1098 1.0" />
	        </material>
	    </visual>
	</static_object>



	<light name="light1" type="directional">
		<position xyz="-2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="-2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="-2.0 -0.6 0.4" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 -0.8 0.4" />
	</camera>

	<!-- TODO: support stereo specification -->

</world>
